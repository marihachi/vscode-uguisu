{
  "name": "vscode-uguisu",
  "displayName": "Uguisu",
  "publisher": "marihachi",
  "description": "Uguisu language support for Visual Studio Code",
  "version": "0.8.0-alpha.1",
  "author": "Marihachi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marihachi/vscode-uguisu.git"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "uguisu",
        "extensions": [
          ".ug"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "uguisu",
        "scopeName": "source.uguisu",
        "path": "./syntaxes/uguisu.tmGrammar.json"
      }
    ]
  }
}
